Storage {
  Name = bareos-sd
  Maximum Concurrent Jobs = {{ bareos_storage_max_concurrent_jobs }}
  Maximum Bandwidth Per Job = {{ bareos_storage_max_bandwidth_per_job }}
  Maximum Connections = {{ bareos_storage_maximum_connections }}
  TLS Enable = {{ bareos_storage_tls_enable }}

  # remove comment from "Plugin Directory" to load plugins from specified directory.
  # if "Plugin Names" is defined, only the specified plugins will be loaded,
  # otherwise all storage plugins (*-sd.so) from the "Plugin Directory".
  #
  # Plugin Directory = "/usr/lib64/bareos/plugins"
  # Plugin Names = ""
}
